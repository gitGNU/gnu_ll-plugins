@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix ll: <http://ll-plugins.nongnu.org/lv2/namespace#>.
@prefix llext: <http://ll-plugins.nongnu.org/lv2/ext/>.
@prefix pr: <http://ll-plugins.nongnu.org/lv2/dev/presets#>.


<http://ll-plugins.nongnu.org/lv2/dev/convolver/0>
  a lv2:Plugin, lv2:ReverbPlugin;
  lv2:binary <convolver.so>;
  doap:name "Convolver";
  doap:license <http://usefulinc.com/doap/licenses/gpl>;
  lv2:optionalFeature llext:saverestore;
  ll:pegName "c";
  	
  lv2:port [
    a lv2:AudioPort, lv2:InputPort;
    lv2:index 0;
    lv2:symbol "input";
    lv2:name "Input";
  ],
  
  [
    a lv2:AudioPort, lv2:OutputPort;
    lv2:index 1;
    lv2:symbol "left_output";
    lv2:name "Output (L)";
  ],

  [
    a lv2:AudioPort, lv2:OutputPort;
    lv2:index 2;
    lv2:symbol "right_output";
    lv2:name "Output (R)";
  ].
