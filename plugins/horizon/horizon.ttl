@prefix lv2:  <http://lv2plug.in/ontology#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix ll: <http://ll-plugins.nongnu.org/lv2/namespace#>.
@prefix llext: <http://ll-plugins.nongnu.org/lv2/ext/>.
@prefix gg: <http://ll-plugins.nongnu.org/lv2/ext/gtk2gui#>.

<http://ll-plugins.nongnu.org/lv2/dev/horizon/0.0.0>
  a lv2:Plugin;
  doap:name "Horizon";
  doap:license <http://usefulinc.com/doap/licenses/gpl>;
  lv2:requiredHostFeature ll:dont-use-this-extension;
  ll:pegName "h";  
  gg:gui <http://ll-plugins.nongnu.org/lv2/dev/horizon/0.0.0/gui>;

  lv2:port [
    a llext:MidiPort, lv2:InputPort;
    lv2:index 0;
    lv2:symbol "midi_input";
    lv2:name "MIDI input";
  ],

  [
    a lv2:AudioPort, lv2:OutputPort;
    lv2:index 1;
    lv2:symbol "left";
    lv2:name "Left";
  ],

  [
    a lv2:AudioPort, lv2:OutputPort;
    lv2:index 2;
    lv2:symbol "right";
    lv2:name "Right";
  ].

<http://ll-plugins.nongnu.org/lv2/dev/horizon/0.0.0/gui> gg:binary <horizon_gtk.so>.
