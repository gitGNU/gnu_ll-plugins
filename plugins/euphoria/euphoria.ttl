@prefix lv2:  <http://lv2plug.in/ontology#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix ll: <http://ll-plugins.nongnu.org/lv2/namespace#>.

<http://ll-plugins.nongnu.org/lv2/dev/euphoria/0.0.0>
  a lv2:Plugin;
  doap:name "Euphoria";
  doap:licence <http://usefulinc.com/doap/licenses/gpl>;
  ll:standaloneGui <euphoria_gtk>;
  lv2:optionalHostFeature ll:instrument-ext;

  lv2:port [
    a lv2:ControlRateInputPort;
    lv2:datatype ll:miditype;
    lv2:index 0;
    lv2:symbol "midi_input";
    lv2:name "MIDI input";
  ],

  [
    a lv2:ControlRateInputPort;
    lv2:datatype lv2:float;
    lv2:index 1;
    lv2:symbol "shape";
    lv2:name "Shape";
    lv2:minimum 0;
    lv2:maximum 5;
    lv2:default 1;
    ll:defaultMidiController [
      ll:controllerType ll:CC;
      ll:controllerNumber 1;
    ];
  ],

  [
    a lv2:ControlRateInputPort;
    lv2:datatype lv2:float;
    lv2:index 2;
    lv2:symbol "timbre";
    lv2:name "Timbre";
    lv2:minimum 0;
    lv2:maximum 1;
    lv2:default 0.5;
    ll:defaultMidiController [
      ll:controllerType ll:CC;
      ll:controllerNumber 2;
    ];
  ],

  [
    a lv2:ControlRateInputPort;
    lv2:datatype lv2:float;
    lv2:index 3;
    lv2:symbol "spread";
    lv2:name "Spread";
    lv2:minimum 0;
    lv2:maximum 0.1;
    lv2:default 0.001;
    ll:defaultMidiController [
      ll:controllerType ll:CC;
      ll:controllerNumber 3;
    ];
  ],

  [
    a lv2:ControlRateInputPort;
    lv2:datatype lv2:float;
    lv2:index 4;
    lv2:symbol "width";
    lv2:name "Width";
    lv2:minimum 0;
    lv2:maximum 0.1;
    lv2:default 0.001;
    ll:defaultMidiController [
      ll:controllerType ll:CC;
      ll:controllerNumber 4;
    ];
  ],

  [
    a lv2:ControlRateInputPort;
    lv2:datatype lv2:float;
    lv2:index 5;
    lv2:symbol "gain_mod";
    lv2:name "Gain modulation";
    lv2:minimum -1;
    lv2:maximum 1;
    lv2:default 0.5;
    ll:defaultMidiController [
      ll:controllerType ll:CC;
      ll:controllerNumber 5;
    ];
  ],

  [
    a lv2:ControlRateInputPort;
    lv2:datatype lv2:float;
    lv2:index 6;
    lv2:symbol "freq_mod";
    lv2:name "Frequency modulation";
    lv2:minimum -1;
    lv2:maximum 10;
    lv2:default 0;
    ll:defaultMidiController [
      ll:controllerType ll:CC;
      ll:controllerNumber 6;
    ];
  ],

  [
    a lv2:ControlRateInputPort;
    lv2:datatype lv2:float;
    lv2:index 7;
    lv2:symbol "timbre_mod";
    lv2:name "Timbre modulation";
    lv2:minimum -1;
    lv2:maximum 1;
    lv2:default 1.0;
    ll:defaultMidiController [
      ll:controllerType ll:CC;
      ll:controllerNumber 7;
    ];
  ],


  [
    a lv2:ControlRateInputPort;
    lv2:datatype lv2:float;
    lv2:index 8;
    lv2:symbol "decay";
    lv2:name "Decay";
    lv2:minimum 0.999;
    lv2:maximum 1.0;
    lv2:default 0.99999;
    ll:defaultMidiController [
      ll:controllerType ll:CC;
      ll:controllerNumber 8;
    ];
  ],


  [
    a lv2:ControlRateInputPort;
    lv2:datatype lv2:float;
    lv2:index 9;
    lv2:symbol "release";
    lv2:name "Release";
    lv2:minimum 0.01;
    lv2:maximum 10;
    lv2:default 0.5;
    ll:defaultMidiController [
      ll:controllerType ll:CC;
      ll:controllerNumber 9;
    ];
  ],


  [
    a lv2:AudioRateOutputPort;
    lv2:datatype lv2:float;
    lv2:index 10;
    lv2:symbol "left";
    lv2:name "Left";
  ],

  [
    a lv2:AudioRateOutputPort;
    lv2:datatype lv2:float;
    lv2:index 11;
    lv2:symbol "right";
    lv2:name "Right";
  ].

