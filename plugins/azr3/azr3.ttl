@prefix lv2:  <http://lv2plug.in/ontology#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix ll: <http://ll-plugins.nongnu.org/lv2/namespace#>.

<http://ll-plugins.nongnu.org/lv2/dev/azr3/0.0.0>
  a lv2:Plugin;
  doap:name "AZR3";
  doap:licence <http://usefulinc.com/doap/licenses/gpl>;
  ll:standaloneGui <azr3_gtk>;
    
  lv2:port [
    a lv2:ControlRateInputPort;
    lv2:datatype lv2:float;
    lv2:index 0;
    lv2:symbol "vol1";
    lv2:name "Vol 1";
    lv2:minimum 0;
    lv2:default 1;
    lv2:maximum 1;
  ],
  
  [
    a lv2:ControlRateInputPort;
    lv2:datatype lv2:float;
    lv2:index 1;
    lv2:symbol "vol2";
    lv2:name "Vol 2";
    lv2:minimum 0;
    lv2:default 1;
    lv2:maximum 1;
  ],
  
  [
    a lv2:ControlRateInputPort;
    lv2:datatype lv2:float;
    lv2:index 2;
    lv2:symbol "vol3";
    lv2:name "Vol 3";
    lv2:minimum 0;
    lv2:default 1;
    lv2:maximum 1;
  ],
  
  [
    a lv2:ControlRateInputPort;
    lv2:datatype lv2:float;
    lv2:index 3;
    lv2:symbol "master";
    lv2:name "Master";
    lv2:minimum 0;
    lv2:default 1;
    lv2:maximum 1;
  ],
  
  [
    a lv2:ControlRateInputPort;
    lv2:datatype lv2:float;
    lv2:index 4;
    lv2:symbol "mrvalve";
    lv2:name "Mr Valve";
    lv2:minimum 0;
    lv2:default 1;
    lv2:maximum 1;
  ],
  
  [
    a lv2:ControlRateInputPort;
    lv2:datatype lv2:float;
    lv2:index 5;
    lv2:symbol "drive";
    lv2:name "Drive";
    lv2:minimum 0;
    lv2:default 1;
    lv2:maximum 1;
  ],
  
  [
    a lv2:ControlRateInputPort;
    lv2:datatype lv2:float;
    lv2:index 6;
    lv2:symbol "set";
    lv2:name "Set";
    lv2:minimum 0;
    lv2:default 1;
    lv2:maximum 1;
  ],
  
  [
    a lv2:ControlRateInputPort;
    lv2:datatype lv2:float;
    lv2:index 7;
    lv2:symbol "tone";
    lv2:name "Tone";
    lv2:minimum 0;
    lv2:default 1;
    lv2:maximum 1;
  ],
  
  [
    a lv2:ControlRateInputPort;
    lv2:datatype lv2:float;
    lv2:index 8;
    lv2:symbol "mix";
    lv2:name "Mix";
    lv2:minimum 0;
    lv2:default 1;
    lv2:maximum 1;
  ],
  
  [
    a lv2:ControlRateInputPort;
    lv2:datatype lv2:float;
    lv2:index 9;
    lv2:symbol "speakers";
    lv2:name "Speakers";
    lv2:minimum 0;
    lv2:default 1;
    lv2:maximum 1;
  ],
  
  [
    a lv2:ControlRateInputPort;
    lv2:datatype lv2:float;
    lv2:index 10;
    lv2:symbol "speed";
    lv2:name "Speed";
    lv2:minimum 0;
    lv2:default 0;
    lv2:maximum 1;
    ll:defaultMidiController [
      ll:controllerType ll:CC;
      ll:controllerNumber 1;
    ];
  ],
  
  [
    a lv2:ControlRateInputPort;
    lv2:datatype lv2:float;
    lv2:index 11;
    lv2:symbol "l_slow";
    lv2:name "Lower slow";
    lv2:minimum 0;
    lv2:default 0.05;
    lv2:maximum 1;
  ],
  
  [
    a lv2:ControlRateInputPort;
    lv2:datatype lv2:float;
    lv2:index 12;
    lv2:symbol "l_fast";
    lv2:name "Lower fast";
    lv2:minimum 0;
    lv2:default 0.75;
    lv2:maximum 1;
  ],
  
  [
    a lv2:ControlRateInputPort;
    lv2:datatype lv2:float;
    lv2:index 13;
    lv2:symbol "u_slow";
    lv2:name "Upper slow";
    lv2:minimum 0;
    lv2:default 0.05;
    lv2:maximum 1;
  ],
  
  [
    a lv2:ControlRateInputPort;
    lv2:datatype lv2:float;
    lv2:index 14;
    lv2:symbol "u_fast";
    lv2:name "Upper fast";
    lv2:minimum 0;
    lv2:default 0.75;
    lv2:maximum 1;
  ],
  
  [
    a lv2:ControlRateInputPort;
    lv2:datatype lv2:float;
    lv2:index 15;
    lv2:symbol "belt";
    lv2:name "Belt";
    lv2:minimum 0;
    lv2:default 0.5;
    lv2:maximum 1;
  ],
  
  [
    a lv2:ControlRateInputPort;
    lv2:datatype lv2:float;
    lv2:index 16;
    lv2:symbol "spread";
    lv2:name "Spread";
    lv2:minimum 0;
    lv2:default 0.5;
    lv2:maximum 1;
  ],
  
  [
    a lv2:ControlRateInputPort;
    lv2:datatype lv2:float;
    lv2:index 17;
    lv2:symbol "splitpoint";
    lv2:name "Splitpoint";
    lv2:minimum 0;
    lv2:default 0.5;
    lv2:maximum 1;
  ],
  
  [
    a lv2:ControlRateInputPort;
    lv2:datatype lv2:float;
    lv2:index 18;
    lv2:symbol "complex";
    lv2:name "Complex";
    lv2:minimum 0;
    lv2:default 0.5;
    lv2:maximum 1;
  ],
  
  [
    a lv2:ControlRateInputPort;
    lv2:datatype lv2:float;
    lv2:index 19;
    lv2:symbol "pedalspeed";
    lv2:name "Pedal speed";
    lv2:minimum 0;
    lv2:default 0.5;
    lv2:maximum 1;
  ],
  
  [
    a lv2:ControlRateInputPort;
    lv2:datatype ll:miditype;
    lv2:index 20;
    lv2:symbol "midi";
    lv2:name "MIDI input";
  ],
  
  [
    a lv2:AudioRateOutputPort;
    lv2:datatype lv2:float;
    lv2:index 21;
    lv2:symbol "left";
    lv2:name "Left output";
  ],

  [
    a lv2:AudioRateOutputPort;
    lv2:datatype lv2:float;
    lv2:index 22;
    lv2:symbol "right";
    lv2:name "Right output";
  ].

