@prefix lv2:  <http://lv2plug.in/ontology#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.

<http://ll-plugins.nongnu.org/lv2/dev/control2midi/0.0.0>
  a lv2:Plugin;
  doap:name "Control2MIDI";
  doap:license <http://usefulinc.com/doap/licenses/gpl>;

  lv2:port [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 0;
    lv2:symbol "input";
    lv2:name "Input";
    lv2:default 0;
  ],
  
  [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 1;
    lv2:symbol "min";
    lv2:name "Min value";
    lv2:default 0;
  ],

  [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 2;
    lv2:symbol "max";
    lv2:name "Max value";
    lv2:default 1;
  ],

  [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 3;
    lv2:symbol "cc";
    lv2:name "Controller";
    lv2:default 0;
  ],

  [
    a <http://ll-plugins.nongnu.org/lv2/ext/MidiPort>, lv2:OutputPort;
    lv2:index 4;
    lv2:symbol "output";
    lv2:name "Output";
  ].

