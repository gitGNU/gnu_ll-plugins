@prefix lv2:  <http://lv2plug.in/ontology#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.

<http://ll-plugins.nongnu.org/lv2/dev/phase-distortion-osc/0.0.0>
  a lv2:Plugin;
  doap:name "Phase distortion oscillator";
  doap:licence <http://usefulinc.com/doap/licenses/gpl>;

  lv2:port [
    a lv2:AudioRateInputPort;
    lv2:datatype lv2:float;
    lv2:index 0;
    lv2:symbol "frequency";
    lv2:name "Frequency (Hz)";
    lv2:default 440;
    lv2:minimum 0;
    lv2:maximum 20000;
  ],
  
  [
    a lv2:ControlRateInputPort;
    lv2:datatype lv2:float;
    lv2:index 1;
    lv2:symbol "shape";
    lv2:name "Shape";
    lv2:default 0;
    lv2:minimum 0;
    lv2:maximum 5;
  ],

  [
    a lv2:ControlRateInputPort;
    lv2:datatype lv2:float;
    lv2:index 2;
    lv2:symbol "amount";
    lv2:name "Amount";
    lv2:default 1;
    lv2:minimum 0;
    lv2:maximum 1;
  ],

  [
    a lv2:AudioRateOutputPort;
    lv2:datatype lv2:float;
    lv2:index 3;
    lv2:symbol "output";
    lv2:name "Output";
  ].
