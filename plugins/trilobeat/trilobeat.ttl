@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix ll: <http://ll-plugins.nongnu.org/lv2/namespace#>.
@prefix ev: <http://lv2plug.in/ns/ext/event#>.
@prefix cx: <http://ll-plugins.nongnu.org/lv2/dev/contexts/1#>.

<http://ll-plugins.nongnu.org/lv2/dev/trilobeat/0>
  a lv2:Plugin;
  doap:name "Trilobeat";
  doap:license <http://usefulinc.com/doap/licenses/gpl>;
  lv2:requiredFeature <http://ll-plugins.nongnu.org/lv2/dev/contexts/1>;
  ll:gtk2Gui <trilobeat_gtk.so>;
  ll:pegName "k";  
  
  lv2:port [
    a lv2:AudioPort, lv2:InputPort;
    lv2:index 0;
    lv2:symbol "trigger";
    lv2:name "Trigger";
  ],

  [
    a lv2:AudioPort, lv2:InputPort;
    lv2:index 1;
    lv2:symbol "gate";
    lv2:name "Gate";
  ],

  [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 2;
    lv2:symbol "bpm";
    lv2:name "BPM";
    lv2:default 120;
  ],

  [
    a ev:EventPort, lv2:OutputPort;
    ev:supportsEvent <http://lv2plug.in/ns/ext/midi#MidiEvent>;
    lv2:index 3;
    lv2:symbol "midi_out";
    lv2:name "MIDI out";
  ];

  cx:requiredContext [
    a cx:MessageContext;
    cx:port [
      a ev:EventPort, lv2:InputPort;
      ev:supportsEvent <http://lv2plug.in/ns/ext/osc#OscEvent>;
      lv2:index 4;
      lv2:symbol "msg_input";
      lv2:name "Message input";
    ];
  ].
