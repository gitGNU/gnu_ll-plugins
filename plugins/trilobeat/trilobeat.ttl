@prefix lv2:  <http://lv2plug.in/ontology#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix ll: <http://ll-plugins.nongnu.org/lv2/namespace#>.
@prefix llext: <http://ll-plugins.nongnu.org/lv2/ext/>.

<http://ll-plugins.nongnu.org/lv2/dev/trilobeat/0.0.0>
  a lv2:Plugin;
  doap:name "Trilobeat";
  doap:licence <http://usefulinc.com/doap/licenses/gpl>;
  lv2:requiredHostFeature ll:instrument-ext;
  ll:gtk2Gui <trilobeat_gtk.so>;
  ll:pegName "k";  
  
  lv2:port [
    a lv2:AudioPort, lv2:InputPort;
    lv2:index 0;
    lv2:symbol "trigger";
    lv2:name "Trigger";
  ],

  [
    a lv2:AudioPort, lv2:InputPort;
    lv2:index 1;
    lv2:symbol "gate";
    lv2:name "Gate";
  ],

  [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 2;
    lv2:symbol "bpm";
    lv2:name "BPM";
    lv2:default "120";
  ],

  [
    a llext:MidiPort, lv2:OutputPort;
    lv2:index 3;
    lv2:symbol "midi_out";
    lv2:name "MIDI out";
  ].
