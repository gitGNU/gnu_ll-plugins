@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix guiext: <http://ll-plugins.nongnu.org/lv2/ext/gui#>.

<http://ll-plugins.nongnu.org/lv2/dev/gcf/0>
  a lv2:Plugin;
  doap:name "GCF";
  doap:license <http://usefulinc.com/doap/licenses/gpl>;
  guiext:gui <http://ll-plugins.nongnu.org/lv2/dev/gcf/0/gui>;
  
  lv2:port [
    a lv2:AudioPort, lv2:InputPort;
    lv2:index 0;
    lv2:symbol "input";
    lv2:name "Input";
  ],

  [
    a lv2:ControlPort, lv2:OutputPort;
    lv2:index 1;
    lv2:symbol "output";
    lv2:name "Output";
  ].

<http://ll-plugins.nongnu.org/lv2/dev/gcf/0/gui> 
  a guiext:GtkGUI;
  guiext:binary <gcf_gtk.so>;
  guiext:portNotification 
    [ guiext:plugin <http://ll-plugins.nongnu.org/lv2/dev/gcf/0>;
      guiext:portIndex 1 ].
