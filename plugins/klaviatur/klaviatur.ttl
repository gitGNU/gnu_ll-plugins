@prefix lv2:  <http://lv2plug.in/ontology#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix ll: <http://ll-plugins.nongnu.org/lv2/namespace#>.
@prefix llext: <http://ll-plugins.nongnu.org/lv2/ext/>.
@prefix gg: <http://ll-plugins.nongnu.org/lv2/ext/gtk2gui#>.

<http://ll-plugins.nongnu.org/lv2/dev/klaviatur/0.0.0>
  a lv2:Plugin;
  doap:name "Klaviatur";
  doap:licence <http://usefulinc.com/doap/licenses/gpl>;
  gg:gui <http://ll-plugins.nongnu.org/lv2/dev/klaviatur/0.0.0/gui>;
  ll:pegName "k";  
  
  lv2:port [
    a llext:MidiPort, lv2:InputPort;
    lv2:index 0;
    lv2:symbol "midi_input";
    lv2:name "MIDI input";
  ],

  [
    a llext:MidiPort, lv2:OutputPort;
    lv2:index 1;
    lv2:symbol "midi_output";
    lv2:name "MIDI output";
  ].

<http://ll-plugins.nongnu.org/lv2/dev/klaviatur/0.0.0/gui> 
  gg:binary <klaviatur_gtk.so>.
