@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix ll: <http://ll-plugins.nongnu.org/lv2/namespace#>.
@prefix llext: <http://ll-plugins.nongnu.org/lv2/ext/>.
@prefix ev: <http://lv2plug.in/ns/ext/event#>.
@prefix guiext: <http://ll-plugins.nongnu.org/lv2/ext/ui#>.

<http://ll-plugins.nongnu.org/lv2/dev/klaviatur/0.0.0>
  a lv2:Plugin;
  doap:name "Klaviatur";
  doap:license <http://usefulinc.com/doap/licenses/gpl>;
  guiext:ui <http://ll-plugins.nongnu.org/lv2/dev/klaviatur/0.0.0/gui>;
  ll:pegName "k";  
  
  lv2:port [
    a ev:EventPort, lv2:InputPort;
    lv2:index 0;
    ev:supportsEvent <http://lv2plug.in/ns/ext/midi#MidiEvent>;
    lv2:symbol "midi_input";
    lv2:name "MIDI input";
  ],

  [
    a ev:EventPort, lv2:OutputPort;
    lv2:index 1;
    ev:supportsEvent <http://lv2plug.in/ns/ext/midi#MidiEvent>;
    lv2:symbol "midi_output";
    lv2:name "MIDI output";
  ].

<http://ll-plugins.nongnu.org/lv2/dev/klaviatur/0.0.0/gui> 
  a guiext:GtkUI;
  guiext:requiredFeature guiext:makeResident;
  guiext:binary <klaviatur_gtk.so>;
  guiext:optionalFeature guiext:noUserResize.
