@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix ev: <http://lv2plug.in/ns/ext/event#>.

<http://ll-plugins.nongnu.org/lv2/dev/arpeggiator/0.0.0>
  a lv2:Plugin;
  doap:name "Arpeggiator";
  doap:license <http://usefulinc.com/doap/licenses/gpl>;

  lv2:port [
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 0;
    lv2:symbol "notes_per_second";
    lv2:name "Notes per second";
    lv2:min 0.5;
    lv2:max 8;
    lv2:default 4;
  ],
  
  [
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 1;
    lv2:symbol "pattern_length";
    lv2:name "Pattern length";
    lv2:default 4;
  ],

  [
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 2;
    lv2:symbol "direction";
    lv2:name "Direction";
    lv2:default 1;
  ],

  [
    a lv2:InputPort, ev:EventPort;
    lv2:index 3;
    ev:supportsEvent <http://lv2plug.in/ns/ext/midi#MidiEvent>;
    lv2:symbol "input";
    lv2:name "MIDI input";
  ],

  [
    a lv2:OutputPort, ev:EventPort;
    ev:supportsEvent <http://lv2plug.in/ns/ext/midi#MidiEvent>;
    lv2:index 4;
    lv2:symbol "output";
    lv2:name "MIDI output";
  ].

