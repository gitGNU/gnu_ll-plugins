@prefix lv2:  <http://lv2plug.in/ontology#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix ll: <http://ll-plugins.nongnu.org/lv2/namespace#>.
@prefix llext: <http://ll-plugins.nongnu.org/lv2/ext/>.

<http://ll-plugins.nongnu.org/lv2/dev/trilobyte/0.0.0>
  a lv2:Plugin;
  doap:name "Trilobyte";
  doap:licence <http://usefulinc.com/doap/licenses/gpl>;
  ll:gtk2Gui <trilobyte_gtk.so>;
  ll:pegName "k";  
  
  lv2:port [
    a lv2:AudioRateInputPort;
    lv2:datatype lv2:float;
    lv2:index 0;
    lv2:symbol "trigger";
    lv2:name "Trigger";
  ],

  [
    a lv2:AudioRateInputPort;
    lv2:datatype lv2:float;
    lv2:index 1;
    lv2:symbol "gate";
    lv2:name "Gate";
  ],

  [
    a lv2:AudioRateInputPort;
    lv2:datatype lv2:float;
    lv2:index 2;
    lv2:symbol "bpm";
    lv2:name "BPM";
  ],

  [
    a lv2:AudioRateOutputPort;
    lv2:datatype lv2:float;
    lv2:index 3;
    lv2:symbol "frequency_out";
    lv2:name "Frequency";
  ],

  [
    a lv2:AudioRateOutputPort;
    lv2:datatype lv2:float;
    lv2:index 4;
    lv2:symbol "trigger_out";
    lv2:name "Trigger";
  ],

  [
    a lv2:AudioRateOutputPort;
    lv2:datatype lv2:float;
    lv2:index 5;
    lv2:symbol "gate_out";
    lv2:name "Gate";
  ].
