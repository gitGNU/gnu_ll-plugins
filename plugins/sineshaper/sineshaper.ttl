@prefix lv2:  <http://lv2plug.in/ontology#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix ll: <http://ll-plugins.nongnu.org/lv2/namespace#>.
@prefix llext: <http://ll-plugins.nongnu.org/lv2/ext/>.

<http://ll-plugins.nongnu.org/lv2/dev/sineshaper/0.0.0>
  a lv2:Plugin;
  doap:name "Sineshaper";
  doap:licence <http://usefulinc.com/doap/licenses/gpl>;
  ll:standaloneGui <sineshaper_gtk>;
  lv2:requiredHostFeature ll:instrument-ext;
  ll:defaultPresets <presets.ttl>;
  
  lv2:port [
    a lv2:ControlRateInputPort;
    lv2:datatype lv2:float;
    lv2:index 0;
    lv2:symbol "tun";
    lv2:name "Tune";
    lv2:minimum 0.5;
    lv2:maximum 2;
    lv2:default 1;
    ll:defaultMidiController [
      ll:controllerType ll:CC;
      ll:controllerNumber 16;
    ];
  ],
  
  [
    a lv2:ControlRateInputPort;
    lv2:datatype lv2:float;
    lv2:index 1;
    lv2:symbol "oct";
    lv2:name "Octave";
    lv2:minimum -10;
    lv2:maximum 10;
    lv2:default 0;
    ll:defaultMidiController [
      ll:controllerType ll:CC;
      ll:controllerNumber 17;
    ];
  ],
  
  [
    a lv2:ControlRateInputPort;
    lv2:datatype lv2:float;
    lv2:index 2;
    lv2:symbol "sub_tun";
    lv2:name "Sub oscillator tune";
    lv2:minimum 0.5;
    lv2:maximum 2;
    lv2:default 0.94;
    ll:defaultMidiController [
