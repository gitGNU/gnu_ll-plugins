@prefix lv2:  <http://lv2plug.in/ontology#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.

<http://ll-plugins.nongnu.org/lv2/dev/midi_identity/0.0.0>
  a lv2:Plugin;
  doap:name "Identity (MIDI)";
  doap:licence <http://usefulinc.com/doap/licenses/gpl>;

  lv2:port [
    a lv2:ControlRateInputPort;
    lv2:datatype <http://ll-plugins.nongnu.org/lv2/namespace#miditype>;
    lv2:index 0;
    lv2:symbol "input";
    lv2:name "Input";
  ],
  
  [
    a lv2:ControlRateOutputPort;
    lv2:datatype <http://ll-plugins.nongnu.org/lv2/namespace#miditype>;
    lv2:index 1;
    lv2:symbol "output";
    lv2:name "Output";
  ].
